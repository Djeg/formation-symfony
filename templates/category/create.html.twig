{% extends 'admin/base.html.twig' %}

{% block page_title %}
	Creer une catégorie
{% endblock %}

{% block content %}
	<p>
		<a href="{{ path('app_category_list') }}">Retour à la liste des catégories</a>
	</p>

	<form method="POST">
		<div>
			<input name="name" type="text" placeholder="Nom de la catégorie ..."/>
		</div>
		<div>
			<button type="submit">Envoyer</button>
		</div>
	</form>
{% endblock %}
